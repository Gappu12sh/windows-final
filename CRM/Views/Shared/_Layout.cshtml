    <style>
        #pluswrap {
            position: fixed;
            width: 100%;
            height: 100%;
            display: flex;
            align-items: center;
            background: rgba(0, 0, 0, 0.21);
            top: 0;
            z-index: 2222202930;
        }

        body {
            margin: 0;
            /*new css*/
            background-color: #ffffff;
            font-family: 'Montserrat',sans-serif;
            /*new css*/
        }

        .plus {
            display: flex;
            margin: 0 auto;
        }

        .logo {
            color: #191f2a;
            display: block;
            width: 100%;
            text-align: center !important;
            margin-bottom: 10px;
        }

        #sidebar-menu .menu_section.active {
            background-color: #28313a !important;
        }

        .nav.side-menu > li.active {
            background-color: #28313a !important;
        }

        .main_menu .fa-file-text-o {
            width: 13px !important;
            font-size: 13px !important;
        }

        .nav-md.page-dashboard {
            padding-right: 0px !important;
        }
    </style>
</head>
