    <script language="javascript" type="text/javascript">
        var userId = '@ViewBag.UserInfo.Id';
        var RequestedUserId = '@ViewBag.RequestedUserId';
        function RedirectToLoginOnSuccess(response) {
            var responsesuccessLogin;
            if (response != '') {
                if (typeof (response) === 'string') {
                    responsesuccessLogin = response;
                }
                else {
                    responsesuccessLogin = JSON.stringify(response);
                }
                if (responsesuccessLogin.indexOf('logo-login.png') > 0) {
                    location.reload(true);
                }
            }

        }
        function RedirectToLoginOnError(response) {
            var responseErrorLogin;
            if (response != '') {
                if (typeof (response) === 'string') {
                    responsesuccessLogin = response;
                }
                else {
                    responseErrorLogin = JSON.stringify(response);
                }
                if (responseErrorLogin.indexOf('logo-login.png') > 0) {
                    location.reload(true);
                }
            }
        }
        function HideProgress() {
            $("#pluswrap").addClass("hide");
        }
        function ShowProgress() {
            $("#pluswrap").removeClass("hide");
        }
        function financial(x) {
            return Number.parseFloat(x).toFixed(3);
        }
    </script>
